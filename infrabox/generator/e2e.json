{
    "version": 1,
    "jobs": [{
        "type": "docker",
        "name": "e2e-k8s-1-15",
        "docker_file": "infrabox/test/e2e/Dockerfile",
        "build_context": "../..",
        "build_only": false,
        "resources": {
            "limits": { "cpu": 1, "memory": 2048 }
        },
        "cache": {
            "image": true
        },
        "timeout": 2700,
        "services": [{
            "apiVersion": "gcp.service.infrabox.net/v1alpha1",
            "kind": "GKECluster",
            "metadata": {
                "name": "e2e-cluster"
            },
            "spec": {
                "machineType": "n1-standard-4",
                "numNodes": 2,
                "preemptible": false,
                "zone": "us-east1-b",
                "clusterVersion": "1.15"
            }
        }]
    }]
}
